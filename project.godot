config_version=5

[application]

config/name="IWTS"
config/version="0.1"
run/main_scene="res://levels/main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/image="res://sprites/Sprite-0003.png"
config/icon="res://sprites/Sprite-0003 (2).png"

[autoload]

Dialogic="*res://addons/dialogic/Other/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"npc1": "res://objects/npc1.dch",
"npc2": "res://objects/npc2.dch",
"steev": "res://objects/steev.dch"
}
directories/dtl_directory={
"npc1-1": "res://dialog/npc1-1.dtl",
"npc2-1": "res://dialog/npc2-1.dtl",
"objects/npc1-1": "res://objects/npc1-1.dtl"
}
glossary/default_case_sensitive=true
variables={}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
layout/style_list=["res://dialog/style1.tres", "res://fonts/UbuntuMono/teststyle.tres"]
layout/default_style="res://dialog/style1.tres"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/initial_position_type=4
window/size/resizable=false
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="IWTS"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/dialogic/plugin.cfg")

